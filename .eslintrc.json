{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "no-plusplus": 0,
        "no-loop-func": 0,
        "no-use-before-define": 0,
        "import/prefer-default-export": 0,
        "no-shadow": 0,
        "global-require": 0,
        "no-undef": 0,
        "max-len": 0
    }
}
